    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "^3.7.4",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.9.3"
