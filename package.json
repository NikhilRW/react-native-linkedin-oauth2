    "check-format": "prettier --check .",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "keywords": [
    "linkedin",
    "oauth2",
    "react-native"
  ],
  "author": "Nikhil Wankhede",
  "homepage": "https://github.com/NikhilRW/react-native-linkedin-oauth2",
  "bugs": {
    "url": "https://github.com/NikhilRW/react-native-linkedin-oauth2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikhilRW/react-native-linkedin-oauth2.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@changesets/cli": "^2.29.8",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
